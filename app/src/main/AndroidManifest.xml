<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rentcars">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/rent"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RentCars">
        <activity android:name=".Cabinet"></activity>
        <activity
            android:name=".CarsShowChange"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CarsShow"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SignUp"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UpdateDelete"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddCars"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>